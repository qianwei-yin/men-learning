        "express-rate-limit": "^6.7.0",
        "helmet": "^6.0.1",
        "hpp": "^0.2.3",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^5.13.15",
        "morgan": "^1.10.0",
        "nodemailer": "^6.9.1",
        "pug": "^3.0.2",
        "slugify": "^1.6.5",
        "validator": "^13.7.0",
        "xss-clean": "^0.1.1"
